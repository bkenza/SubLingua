{
  "expo": {
    "name": "SubLingua",
    "slug": "SubLingua",
    "privacy": "public",
    "description": "SubLingua is a simple translation mobile application that uses object recognition. It was built using React Native and the Clarifai API. The current version is a beta version.",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.soen357.sublingua",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.soen357.sublingua",
      "versionCode": 1
    }
  }
}